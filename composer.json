{
    "name": "sikessem/sikessem.com",
    "type": "project",
    "description": "The SIKessEm Website",
    "homepage": "https://sikessem.com",
    "license": "GPL-3.0-only",
    "keywords": [
        "sikessem",
        "website",
        "application"
    ],
    "scripts": {
        "app": "./ske app",
        "css": "sass ./app/res/styles/:./client/static/ -s compressed --no-source-map"
    },
    "authors": [{
        "name": "SIGUI KessÃ© Emmanuel",
        "homepage": "https://about.sikessem.com",
        "email": "developer@sikessem.com",
        "role": "Developer"
    }],
    "support": {
        "source": "https://github.com/SIKessEm/Website",
        "issues": "https://github.com/SIKessEm/Website/issues"
    },
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src/core/",
		  	"Cli\\": "app/src/main/cli/",
		  	"Cgi\\": "app/src/main/cgi/",
            "Ske\\Util\\": "pkg/ske/util/",
		  	"Ske\\Bundle\\": "pkg/ske/bundle/",
			"Ske\\Widget\\": "pkg/ske/widget/",
			"Ske\\": "pkg/ske/core/"
		},
        "files": [
            "app/src/main/cfg/helper.php"
        ]
    },
    "config": {
        "web-dir": "client",
        "vendor-dir": "php_packages",
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": "./ske",
    "minimum-stability": "dev",
    "prefer-stable": true
}
