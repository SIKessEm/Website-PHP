{
    "name": "sikessem/sikessem.com",
    "type": "project",
    "description": "The SIKessEm Website",
    "homepage": "https://sikessem.com",
    "license": "GPL-3.0-only",
    "keywords": [
        "sikessem",
        "website",
        "application"
    ],
    "scripts": {
        "app": "node main.js",
        "css": "sass ./res/styles/:./cgi/static/ -s compressed --no-source-map",
    },
    "authors": [{
        "name": "SIGUI KessÃ© Emmanuel",
        "homepage": "https://about.sikessem.com",
        "email": "developer@sikessem.com",
        "role": "Developer"
    }],
    "support": {
        "source": "https://github.com/SIKessEm/Website",
        "issues": "https://github.com/SIKessEm/Website/issues"
    },
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Ske\\IO\\": "bundle/src/core/pkg/io/src/",
            "Ske\\Cmd\\": "bundle/src/core/pkg/cmd/src/util/",
            "Ske\\Cgi\\": "bundle/src/core/pkg/app/cgi/src/",
            "Ske\\Cli\\": "bundle/src/core/pkg/app/cli/src/",
            "Ske\\": "bundle/src/core/pkg/",
            "Ske\\Util\\": "bundle/src/core/pkg/",
            "SIKessEm\\Util\\": "bundle/src/core/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ske\\Cmd\\": "bundle/src/core/pkg/cmd/src/unit/",
            "Ske\\Unit\\": "bundle/src/test/pkg/",
            "SIKessEm\\Unit\\": "bundle/src/test/app/"
        }
    },
    "replace": {
        "ske/app": "*",
        "ske/cgi": "*",
        "ske/cli": "*",
        "ske/cmd": "*",
        "ske/io": "*"
    },
    "config": {
        "vendor-dir": "php_packages",
        "web-dir": "public",
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": "ske",
    "minimum-stability": "dev",
    "prefer-stable": true
}
