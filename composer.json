{
    "name": "sikessem/sikessem.com",
    "type": "project",
    "description": "A flexible framework for building web apps",
    "homepage": "https://github.com/SIKessEm/App#readme",
    "license": "GPL-3.0-only",
    "keywords": [
        "sikessem",
        "website",
        "application"
    ],
    "scripts": {
        "app": "./ske app",
        "css": "sass ./bundle/res/themes/:./public/static/ -s compressed --no-source-map"
    },
    "authors": [{
        "name": "SIGUI KessÃ© Emmanuel",
        "homepage": "https://about.sikessem.com",
        "email": "developer@sikessem.com",
        "role": "Developer"
    }],
    "support": {
        "source": "https://github.com/SIKessEm/App",
        "issues": "https://github.com/SIKessEm/App/issues"
    },
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Ske\\App\\": "app/src/core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ske\\Test\\": "app/src/test/"
        }
    },
    "config": {
        "vendor-dir": "php_packages",
        "web-dir": "web",
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": "./ske",
    "minimum-stability": "dev",
    "prefer-stable": true
}
